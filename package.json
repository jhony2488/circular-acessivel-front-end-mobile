    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
